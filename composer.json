{
    "name": "kuriousagency/activecampaign",
    "description": "Active Campaign plugin for Craft CMS",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "activecampaign"
    ],
    "support": {
        "docs": "https://github.com/KuriousAgency/activecampaign/blob/master/README.md",
        "issues": "https://github.com/KuriousAgency/activecampaign/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kurious Agency",
            "homepage": "https://kurious.agency"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "kuriousagency\\activecampaign\\": "src/"
        }
    },
    "extra": {
        "name": "ActiveCampaign",
        "handle": "activecampaign",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/KuriousAgency/activecampaign/master/CHANGELOG.md",
        "components": {
            "activeCampaignService": "kuriousagency\\activecampaign\\services\\ActiveCampaignService"
        },
        "class": "kuriousagency\\activecampaign\\ActiveCampaign"
    }
}
